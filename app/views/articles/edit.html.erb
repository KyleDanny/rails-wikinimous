
<div class="page-margin">
  <h1>Editing article</h1>

  <%= simple_form_for(@article) do |f| %>
    <%= f.input :title %>
    <%= f.input :content %>
    <%= f.submit %>
  <% end %>

  <div class="btn btn-link">
    <%=link_to "Show", article_path %> ||
    <%=link_to "Back", articles_path %>
  </div>
</div>
